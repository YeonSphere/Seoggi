// Test script for colored output

// Import the testing framework
include 'testing_framework.seo';
include 'execution.seo';

function testColoredOutput() {
    runTest('Test Colored Output', function() {
        printColored('This is a success message!', COLORS.SUCCESS);
        printColored('This is an error message!', COLORS.ERROR);
        printColored('This is a warning message!', COLORS.WARNING);
        printColored('This is an info message!', COLORS.INFO);
    });
}

function testExecution() {
    runTest('Test Execution', function() {
        const script = 'func testFunc() { return 42; }';
        execute(script);
    });
}

testColoredOutput();
testExecution();
